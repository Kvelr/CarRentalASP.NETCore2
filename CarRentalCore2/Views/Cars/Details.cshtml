@model CarRentalCore2.Models.Car
@{
    ViewData["Title"] = "Create";
}

<br />
<h2>Car Details</h2>
<hr />


<div class="form-border">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <div class="row">
            <div class="col-sm-2">
                <label asp-for="Vin" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" asp-for="Vin" class="form-control" disabled />
            </div>
        </div>

    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-sm-2">
                <label asp-for="Make" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" asp-for="Make" class="form-control" disabled />
            </div>
        </div>

    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-sm-2">
                <label asp-for="Model" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" asp-for="Model" class="form-control" disabled />
            </div>
        </div>

    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-sm-2">
                <label asp-for="Style" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" asp-for="Style" class="form-control" disabled />
            </div>
        </div>

    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-sm-2">
                <label asp-for="Year" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" asp-for="Year" class="form-control" disabled />
            </div>
        </div>

    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-sm-2">
                <label asp-for="Miles" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" asp-for="Miles" class="form-control" disabled />
            </div>
        </div>

    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-sm-2">
                <label asp-for="Color" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" asp-for="Color" class="form-control" disabled />
            </div>
        </div>

    </div>
    <br />
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-sm btn-primary">Edit</a>
        <a asp-action="Index" asp-route-userId="@Model.UserId" class="btn btn-sm btn-success">Back to list</a>
    </div>
</div>



