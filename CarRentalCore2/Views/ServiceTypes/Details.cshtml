@model CarRentalCore2.Models.ServiceType
@{
    ViewData["Title"] = "Details";
}

<br />
<h2>Service Type Details</h2>
<hr />

<div class="form-border">
    <div class="form-group">
        <div class="row">
            <div class="col-sm-1">
                <label asp-for="Name" class="custom-label"></label>
            </div>
            <div class="col-sm-4">
                <input type="text" disabled asp-for="Name" class="form-control" />
            </div>
        </div>
    </div>
    <br />
    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.ID" class="btn btn-sm btn-primary">Edit</a>
        <a asp-action="Index" class="btn btn-sm btn-success">Back to list</a>
    </div>
</div>